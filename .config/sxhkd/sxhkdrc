#Basic Binds
#Passmenu
super + u
	passmenu --type
#Passmenu copy
super + U
	passmenu
#Lock
super + F11
	slock
#Terminal Shortcut
super + Return
	$TERMINAL
#Launch Transmission
super + T 
	transmission-qt
#Launch dmenu
super + p
	dmenu_run

#Launch bookmark + search browser
super + w
	bookmarksurf
#Launch Browser
super + W
	tabbed surf -e	
#Launch Newsboat
super + n
	$TERMINAL -e newsboat

#Launch Music[ncmpcpp]
super + O 
	$TERMINAL -e ncmpcpp

#Launch Music[mpdmenu]
super + o
	mpdmenu

# Audiokeys
XF86AudioMute
	amixer sset Master toggle; 
XF86Audio{Raise,Lower}Volume
	amixer sset Master 5%{+,-}; 
XF86Audio{Next,Prev}
	mpc {next,prev}
XF86Audio{Play}
	mpc {toggle}
